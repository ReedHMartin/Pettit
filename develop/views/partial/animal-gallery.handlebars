{{#each aniaml of |animalPhotos|}}
<section class="card">
  <!-- The contents of this card links to the gallery page using the gallery ID -->
  <a href="/gallery/{{id}}">
    <img src="{{picture}}" alt="{{paintings.[0].description}}" class="card-img-top"/>
    <ul class="list-group" style="width:18rem;">
      <li class="list-group-item">{{name}}</li>
      <li class="list-group-item">{{species}}</li>
      <li class="list-group-item">{{status}}</li>
    </ul>
</section>
